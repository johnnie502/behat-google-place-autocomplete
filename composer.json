{
  "name": "medology/behat-google-place-autocomplete",
  "type": "behat-extension",
  "description": "Mink extension for Google Place Autocomplete",
  "license": "MIT",
  "require": {
    "behat/behat": "^3.0",
    "johnnie502/flexible-mink": "dev-master",
    "behat/mink-extension": "^2.0",
    "symfony/config": "~2.8"
  },
  "require-dev":{
    "vlucas/phpdotenv": "^2.3",
    "behat/mink-selenium2-driver": "*"
  },
  "autoload": {
    "psr-4": {
      "Medology\\": "src/"
    }
  },
  "repositories": [
      {
      "type":"package",
      "package": {
        "name": "johnnie502/flexible-mink",
        "version":"master",
        "source": {
          "url": "https://github.com/johnnie502/FlexibleMink.git",
          "type": "git",
          "reference":"master"
        }
      }
    }
  ]
}

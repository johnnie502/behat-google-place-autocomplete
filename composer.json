{
  "name": "medology/behat-google-place-autocomplete",
  "type": "behat-extension",
  "description": "Mink extension for Google Place Autocomplete",
  "license": "MIT",
  "require": {
    "behat/behat": "^3.0",
    "medology/flexible-mink": "dev-experiment",
    "behat/mink-extension": "dev-experiment",
    "symfony/config": "~2.8"
  },
  "require-dev":{
    "vlucas/phpdotenv": "^2.3",
    "behat/mink-selenium2-driver": "*"
  },
  "autoload": {
    "psr-4": {
      "Medology\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/johnnie502/FlexibleMink"
    },
    {
      "type": "vcs",
      "url": "https://github.com/johnnie502/MinkExtension"
    }
  ]
}
